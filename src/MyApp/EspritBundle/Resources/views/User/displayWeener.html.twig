{% extends '@MyAppEsprit/views/templateAdmin.html.twig' %}
{% block reporttable %}

    <section class="panel">
        <header class="panel-heading custom-tab ">
            <ul class="nav nav-tabs">
                <li class="active">
                    <a href="#home" data-toggle="tab">ALL</a>
                </li>
                <li class="">
                    <a href="#about" data-toggle="tab">Weeners</a>
                </li>
                <li class="">
