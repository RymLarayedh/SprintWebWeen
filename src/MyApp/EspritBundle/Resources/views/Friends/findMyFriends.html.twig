{% extends '@MyAppEsprit/views/templateAdmin.html.twig' %}
{% block reporttable %}
    <!-- page heading start-->
    <div class="page-heading">
        <h3>
            Reports Table
        </h3>
        <ul class="breadcrumb">
            <li>
                <a href="#">Dashboard</a>
            </li>
            <li>
                <a href="#">Data Table</a>
            </li>
            <li class="active"> Reports Table </li>
        </ul>
    </div>
    <!-- page heading end-->

    <!--body wrapper start-->
    <div class="wrapper">
        <div class="row">
            <div class="col-sm-12">
                <section class="panel">
                    <header class="panel-heading">
                        Reports Table
                        <span class="tools pull-right">
                        <a href="javascript:;" class="fa fa-chevron-down"></a>
                        <a href="javascript:;" class="fa fa-times"></a>
                     </span>
                    </header>
                    <div class="panel-body">
                        <div class="adv-table editable-table ">

                            <div class="clearfix">
                                <div class="btn-group">
                                    <button id="editable-sample_new" class="btn btn-primary">
                                        Add New <i class="fa fa-plus"></i>
                                    </button>
                                </div>
                                <div class="btn-group pull-right">
                                    <button class="btn btn-default dropdown-toggle" data-toggle="dropdown">Tools <i class="fa fa-angle-down"></i>
                                    </button>
                                    <ul class="dropdown-menu pull-right">
                                        <li><a href="#">Print</a></li>
                                        <li><a href="{{ path('ween_report_pdf') }}">Save as PDF</a></li>
                                        <li><a href="#">Export to Excel</a></li>
                                    </ul>
                                </div>
                            </div>


                            <!-- ****************** Tableau ******************************** -->
                            <h2>List of My Friends</h2>
                            <table class="table table-striped table-hover table-bordered dataTable"
                                   aria-describedby="editable-sample_info">
                                <thead>
                                <tr role="row">
                                    <th class="sorting_desc" role="columnheader" tabindex="0" aria-controls="editable-sample" rowspan="1" colspan="1" style="width: 169px;" aria-sort="descending" aria-label="Last Name: activate to sort column ascending">User Name</th>
                                    <th class="sorting" role="columnheader" tabindex="0" aria-controls="editable-sample" rowspan="1" colspan="1" style="width: 110px;" aria-label="Points: activate to sort column ascending">Email</th>
                                    <th class="sorting" role="columnheader" tabindex="0" aria-controls="editable-sample" rowspan="1" colspan="1" style="width: 206px;" aria-label="Status: activate to sort column ascending">Nom</th>
                                    <th class="sorting" role="columnheader" tabindex="0" aria-controls="editable-sample" rowspan="1" colspan="1" style="width: 78px;" aria-label="Edit: activate to sort column ascending">Prenom</th>
                                    <th class="sorting" role="columnheader" tabindex="0" aria-controls="editable-sample" rowspan="1" colspan="1" style="width: 112px;" aria-label="Delete: activate to sort column ascending">Last Connection</th>
                                    <th class="sorting" role="columnheader" tabindex="0" aria-controls="editable-sample" rowspan="1" colspan="1" style="width: 112px;" aria-label="Delete: activate to sort column ascending">Delete</th>
                                </tr>
                                </thead>





                                <tbody role="alert" aria-live="polite" aria-relevant="all"><tr class="odd">
                                    {% for m in friends %}
                                <tr>

                                    <td class="">{{ m.friendid.username }}</td>
                                    <td class="">{{ m.friendid.email }}</td>
                                    <td class="">{{ m.friendid.nom }}</td>
                                    <td class="">{{ m.friendid.prenom }}</td>
                                    <td class="">{{ m.friendid.lastLogin |date }}</td>



                                </tr>
                                {% endfor %}
                            </table>
                            {% for m in friends %}
                                <div class="col-md-4">
                                    <div class="panel">
                                        <div class="panel-body">
                                            <div class="media usr-info">
                                                <a class="pull-left" href="#">
                                                    <img alt="" src="{{ asset('bundles/myappesprit/AdminTemplate/images/photos/rym.png') }}" class="thumb">
                                                </a>
                                                <div class="media-body">
                                                    <h4 class="media-heading">{{ m.friendid.username }}</h4>
                                                    <span>Senior UI Designer</span>
                                                    <p>Email : {{ m.friendid.email }}</p>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="panel-footer custom-trq-footer">
                                            <ul class="user-states">
                                                <li>
                                                    <i class="fa fa-heart"></i> 127
                                                </li>
                                                <li>
                                                    <i class="fa fa-eye"></i> 853
                                                </li>
                                                <li>
                                                    <i class="fa fa-user"></i> 311
                                                </li>
                                            </ul>
                                        </div>
                                    </div>


                                </div>
                            {% endfor %}

                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
    <!--body wrapper end-->


{% endblock %}


